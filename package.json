{
  "name": "vue-project",
  "version": "0.0.0",
  "private": true,
  "main": "main.js",
  "homepage": "./",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test:unit": "vitest",
    "start": "electron .",
    "pack": "electron-builder --dir",
    "distwin": "electron-builder -w --publish=never --ia32"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "base64url": "^3.0.1",
    "electron-log": "^3.0.7",
    "electron-prompt": "^1.3.1",
    "electron-store": "^4.0.0",
    "electron-updater": "4.3.5",
    "module-alias": "github:eycorsican/module-alias",
    "sudo-prompt": "github:eycorsican/sudo-prompt",
    "pinia": "^2.0.32",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vue/test-utils": "^2.3.0",
    "electron": "^24.1.2",
    "jsdom": "^21.1.0",
    "vite": "^4.1.4",
    "vitest": "^0.29.1"
  },
  "moduleAliases": {
    "@tiny": "src"
  },
  "build": {
    "appId": "org.tinyvn.id",
    "productName": "Tiny.VN",
    "publish": [
      {
        "provider": "generic",
        "url": "https://tiny.vn/download/dist/"
      }
    ],
    "nsis": {
      "allowElevation": true,
      "perMachine": false,
      "oneClick": true,
      "multiLanguageInstaller": false,
      "warningsAsErrors": true,
      "deleteAppDataOnUninstall": false
    },
    "win": {
      "icon": "src/assets/icon_256.ico",
      "requestedExecutionLevel": "requireAdministrator",
      "certificateSubjectName": "LE TRONG HIEU",
      "signingHashAlgorithms": [
        "sha256"
      ],
      "publisherName": "LE TRONG HIEU",
      "extraResources": [
        {
          "from": "src/helper/win32",
          "to": "src/helper",
          "filter": [
            "core.exe",
            "ensure_tap_device.bat",
            "tap-windows6"
          ]
        },
        {
          "from": "src/helper/win32/tap-windows6",
          "to": "src/helper/tap-windows6",
          "filter": [
            "*"
          ]
        },
        "src/assets"
      ]
    }
  }
}
